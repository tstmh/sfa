                <br /> <br /> <br />
                <h1>
                    <b>
                        Account Created Successfully
                    </b>
                </h1>
                <br />

            </div>
            <br /><br />

            <div class="row">
                <div class="col-md-4">
                </div>
                <div class="col-md-4" style="text-align:center">
                    <button id="btnNext" onclick="@("window.location.href='" + @Url.Action("PreRegistrationLayout", "Home") + "'");" value="" style="border-radius: 8%;background-color:#E9510E;border-color:#E9510E;font-size:24px;cursor: pointer;width:120px;">
                        <b style="color:white;">Login </b>
