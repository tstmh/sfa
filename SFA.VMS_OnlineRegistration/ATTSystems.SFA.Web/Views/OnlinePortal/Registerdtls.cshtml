                                <button type="button" id="btnNRICvrfy" class="btn">Register</button>

                            </div>
                        </div>
                    </fieldset>
                </div>
                <div class="col-md-12" style="display:none" id="nricdtls">
                    <fieldset class="scheduler-border">
