                                </div>
                                <div id="blockNo" style="display:none">
                                    <label class="form-label">Block No</label>
                                    @Html.TextBoxFor(model => model.blockNo, new { @class = "form-control", @autocomplete = "off", id = "nricblock" })
                                </div>

