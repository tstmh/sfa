                        <legend>访客详情</legend>
                        <div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>护照号码</label>
                                        <p id="lstpassport"></p>
                                    </div>
                                    <div class="form-group">
                                        <label>手机号码</label>
                                        <p id="contact"></p>
                                    </div>

                                    <div class="form-group" id="visitor">
                                        <label>访客类型</label>
                                        <p id="visitType"></p>
                                    </div>


                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>姓名</label>
                                        <p id="visitName"></p>
                                    </div>

                                    <div class="form-group" style="display:none" id="vehicleOpt">
