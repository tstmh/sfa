                                </div>

                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>姓名 </label>
                                    @Html.TextBoxFor(model => model.visitorName, new { @class = "form-control", id = "visName", @autocomplete = "off" , oninput = "allowNameOnly(this)"})
                                </div>

