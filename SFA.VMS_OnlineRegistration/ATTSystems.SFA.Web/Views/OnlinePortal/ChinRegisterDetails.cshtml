                                    <datalist id="UIDName">
                                        @foreach (var item in Model.OnlineUnitsDetailLists)
                                        {
                                            <option value=@item.UnitId>@item.UnitId</option>
                                        }
                                    </datalist>
                                </div>

                                <button type="submit" id="btnNRIc" class="btn">登记</button>

                            </div>

                        </div>
                    </fieldset>
                </div>
                <div class="col-md-12" id="myNricDiv" style="display: none;">
                    <fieldset class="scheduler-border">
                        <legend>访客详情</legend>
                        <div>
                            <div class="row">
                                <div class="col-md-6">

                                    <div class="form-group">
                                        <label>身份证/工作准证 号码</label>
                                        <p id="nlstpassport"></p>
                                    </div>
                                    <div class="form-group">
                                        <label>手机号码</label>
                                        <p id="ncontact"></p>
                                    </div>

                                    <div class="form-group" id="visitor">
                                        <label>访客类型</label>
                                        <p id="nvisitType"></p>
                                    </div>

                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>姓名</label>
                                        <p id="nvisitName"></p>
                                    </div>
                                    <div class="form-group" style="display:none" id="nricVehicle">
                                        <label>车牌号码 </label>
                                        <p id="nvehicle"></p>
                                    </div>

                                    <button type="button" id="btnRegisterNric" class="btn">登记</button>
                                   
                                </div>
                            </div>


                        </div>
                    </fieldset>
                </div>
            </div>

            <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                <h3 class="register-heading">使用护照号码注册</h3>
                <div class="col-md-12" id="ckeckPassport">
                    <fieldset class="scheduler-border">
                        <legend>护照号码</legend>
                        <div class="row register-form">
                            <div class="col-md-6">
                                <div class="form-group">
                                    @Html.TextBoxFor(model => model.passportNumber, new { @class = "form-control", id = "passNum", @autocomplete = "off", onkeypress = "allowAlphaNumericPassport(event)", onpaste = "return false;" })
