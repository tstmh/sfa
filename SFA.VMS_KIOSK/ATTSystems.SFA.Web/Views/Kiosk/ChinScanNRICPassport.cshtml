        .modal-content {
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100%;
            width: 80%;
        }

        .modal-dialog {
            display: flex;
            align-items: center;
            justify-content: center;
            min-height: calc(100vh - 1rem);
        }


        @@keyframes spin {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
